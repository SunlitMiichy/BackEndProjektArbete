<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/stylesheets/productsID.css">
    
    <script src="https://kit.fontawesome.com/9bc0025a3d.js" crossorigin="anonymous"></script>

    <title>product-details</title>
</head>
<body>


  <header>
    <%- include('partials/header') %>
  </header>

  <%- include('partials/mainlist') %>

  <main>

    <!-- Hero-->
    <%- include('partials/product-detailsHeroBox') %>

    <!-- Produktbox-->
    <%- include('partials/product-detailsProductBox') %>

    <!-- Produktinformation-->
    <section class="product-info">
      <h1><%= product.productName %></h1>
      <img src="" alt="<%= product.productName %>">
      <p>Pris: <%= product.price %> SEK</p>
    </section>

   
    <%- include('partials/underIcons') %>

  </main>

  <footer class="main-footer">
    <%- include('partials/footer') %>
  </footer>

</body>
</html>